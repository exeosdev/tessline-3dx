

# Matches all 32-bit architectures: ARM, x86
set( _EBS_VINTERNALDEF_MATCH_STR_ARCH_32 "^ARM$|^x86$" )

# Matches all 64-bit architectures: ARM64, x86_64
set( _EBS_VINTERNALDEF_MATCH_STR_ARCH_64 "^ARM64$|^x86_64$" )

# Matches all ARM-based architectures: ARM, ARM64
set( _EBS_VINTERNALDEF_MATCH_STR_ARCH_ARMF "^ARM(64)*$" )

# Matches all x86-based architectures: x86, x86_64
set( _EBS_VINTERNALDEF_MATCH_STR_ARCH_X86F "^x86(_64)*$" )
